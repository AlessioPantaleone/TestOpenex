      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "it": true,
      "window": true,
      "document": true,
      "console": true,
      "localStorage": true,
      "module": true,
      "process": true
    },
    "rules": {
      "no-console": 0
    }
  }
